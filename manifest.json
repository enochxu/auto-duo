{
    "name": "Auto Duo",
    "version": "1.0.4",
    "description": "Automatic Duo Multi-Factor Authentication for UCLA sites.",
    "permissions": [
        "storage",
        "tabs",
        "scripting"
    ],
    "host_permissions": [
        "https://shb.ais.ucla.edu/*",
        "https://api-d594f029.duosecurity.com/*"
    ],
    "action": {
        "default_popup": "popup.html", 
        "default_icon": {
            "16": "images/Icon-16.png",
            "32": "images/Icon-32.png",
            "48": "images/Icon-48.png",
            "128": "images/Icon-128.png"
        }
    },
    "icons": {
        "16": "images/Icon-16.png",
        "32": "images/Icon-32.png",
        "48": "images/Icon-48.png",
        "128": "images/Icon-128.png"
    },
    "manifest_version": 3
}